{
  "name": "stock-dashboard",
  "version": "1.0.0",
  "description": "Playwright test suite for stock dashboard",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:headed_prod": "BASE_URL=https://stock-dashboard-sp500.streamlit.app playwright test --ui",
    "test:staging": "playwright test --project=chromium",
    "test:prod": "BASE_URL=https://stock-dashboard-sp500.streamlit.app playwright test",
    "test:debug": "playwright test --debug",
    "ui": "playwright test --ui",
    "report": "playwright show-report",
    "install:browsers": "playwright install chromium",
    "codegen": "playwright codegen",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Habeeb-MD/stock-dashboard.git"
  },
  "author": "Habeeb-MD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Habeeb-MD/stock-dashboard/issues"
  },
  "homepage": "https://github.com/Habeeb-MD/stock-dashboard#readme",
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^0.22.0"
  }
}